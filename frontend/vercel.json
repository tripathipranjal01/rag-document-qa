{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "app/api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "OPENAI_API_KEY": "@openai_api_key"
  },
  "build": {
    "env": {
      "OPENAI_API_KEY": "@openai_api_key"
    }
  }
}